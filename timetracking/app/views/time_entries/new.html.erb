<% if @time_entry.errors.any? %>
	<ul>
		<% @time_entry.errors.full_messages.each do | error | %>
			<li><%= error %></li>
		<% end %>
	</ul>
<% end %>

<%= form_for [@project, @time_entry] do | f | %>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
	<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>
	<%= f.submit 'Save' %>
<% end %>