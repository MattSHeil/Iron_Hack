<h1>Hey guys these are my users!</h1>

<ul>

	<% @users.each do | user | %>
		<li>
			<%= link_to "#{user.name}", user_path(user) %> 
			- <%= user.email %>
		</li>
		<br>			
	<% end %>

</ul>

<button>
	<%= link_to 'Create New User', new_user_path %>
</button>

<button>
	<%= link_to 'Home', home_path %>
</button>